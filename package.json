{
  "name": "infoteam-idp-be",
  "version": "2.0.0",
  "description": "Infoteam IDP Backend",
  "author": "infoteam",
  "private": true,
  "license": "AGPL-3.0-only",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "bun dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "bun --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config test/e2e/config/jest-e2e.ts",
    "doc": "bun ./patch-compodoc.js && compodoc -p tsconfig.json -a assets -d ./doc",
    "doc:serve": "bun ./patch-compodoc.js && compodoc -p tsconfig.json -a assets -d ./doc -s"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.750.0",
    "@aws-sdk/s3-request-presigner": "^3.750.0",
    "@fastify/cookie": "^11.0.2",
